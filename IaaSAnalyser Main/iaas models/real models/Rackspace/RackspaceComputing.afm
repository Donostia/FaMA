((NOT PaymentDiscounts) AND Computing.grossCostServerMonth >= 10000 AND Computing.grossCostServerMonth < 25000) IMPLIES (Computing.discount == (Computing.grossCostServerMonth - 10000)*0.08 + 200);
((NOT PaymentDiscounts) AND Computing.grossCostServerMonth >= 25000 AND Computing.grossCostServerMonth < 50000) IMPLIES (Computing.discount == (Computing.grossCostServerMonth - 25000)*0.12 + 1400);
((NOT PaymentDiscounts) AND Computing.grossCostServerMonth >= 50000 AND Computing.grossCostServerMonth < 100000) IMPLIES (Computing.discount == (Computing.grossCostServerMonth - 50000)*0.16 + 4400);
((NOT PaymentDiscounts) AND Computing.grossCostServerMonth >= 100000) IMPLIES (Computing.discount == (Computing.grossCostServerMonth - 100000)*0.2 + 12400);

